generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Author {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  bio       String?
  cpf       String
  pais      String?   @db.Char(2)
  createdAt DateTime @default(now()) @map("created_at")

  @@map("authors")
}
